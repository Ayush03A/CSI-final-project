{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CSI-Final-Project-ADF"
		},
		"LS_ADLS_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csifinalprojectadls.dfs.core.windows.net/"
		},
		"LS_REST_CountriesAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/"
		},
		"LS_Synapse_DW_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "csi-final-project-synapse.sql.azuresynapse.net"
		},
		"LS_Synapse_DW_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DWPool"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CSI-Final-Project-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_Gen2_properties_typeProperties_url')]"
					},
					"LS_REST_CountriesAPI_properties_typeProperties_url": {
						"value": "[parameters('LS_REST_CountriesAPI_properties_typeProperties_url')]"
					},
					"LS_Synapse_DW_properties_typeProperties_server": {
						"value": "[parameters('LS_Synapse_DW_properties_typeProperties_server')]"
					},
					"LS_Synapse_DW_properties_typeProperties_database": {
						"value": "[parameters('LS_Synapse_DW_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}